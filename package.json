{
  "name": "xmllm",
  "version": "0.1.6",
  "main": "./dist/cjs/xmllm-main.js",
  "module": "./dist/esm/xmllm-main.mjs",
  "types": "index.d.ts",
  "typesVersions": {
    "*": {
      "client": [
        "./client.d.ts"
      ]
    }
  },
  "files": [
    "src",
    "dist",
    "client.d.ts",
    "index.d.ts",
    "src/**/*"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir dist/cjs --out-file-extension .js",
    "build:esm": "cross-env BABEL_ENV=esm babel src --out-dir dist/esm --out-file-extension .mjs",
    "test": "npm run build && node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:types": "tsd",
    "proxy": "node src/xmllm-run-proxy.mjs"
  },
  "exports": {
    ".": {
      "import": "./src/xmllm-main.mjs",
      "require": "./dist/cjs/xmllm-main.js",
      "types": "./index.d.ts"
    },
    "./client": {
      "import": "./src/xmllm-client.mjs",
      "require": "./dist/cjs/xmllm-client.js",
      "types": "./client.d.ts"
    },
    "./proxy": {
      "import": "./src/xmllm-proxy.mjs",
      "require": "./dist/cjs/xmllm-proxy.js"
    }
  },
  "dependencies": {
    "@types/node": "^22.10.1",
    "cors": "^2.8.5",
    "css-select": "^5.1.0",
    "dotenv": "^16.4.5",
    "eventsource-parser": "^2.0.1",
    "express": "^4.19.2",
    "htmlparser2": "^9.1.0",
    "lru-cache": "^11.0.0",
    "p-queue": "^8.0.1",
    "streamops": "^0.1.20"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.25.4",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-add-import-extension": "^1.6.0",
    "babel-plugin-transform-import-extension": "^1.0.3",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "supertest": "^7.0.0",
    "tsd": "^0.31.2",
    "typescript": "^5.6.3",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "bin": {
    "xmllm-proxy": "./dist/cjs/xmllm-run-proxy.js"
  }
}
